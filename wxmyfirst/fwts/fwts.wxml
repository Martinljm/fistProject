<!--pages/fwts/fwts.wxml-->
<view class='fwts'>
  <view class="tj">
    <image src='/pages/tabs/fwts1.png'></image>
    <text class="t1">为您推荐服务位置</text>
    <text class="t2">请将车停至以下位置附近</text>
  </view>
  <view class="addr" wx:for="{{addrlist}}" wx:key="index">
    <image src="/pages/tabs/fwts2.png" class="img1"></image>
    <view class="addr-t">
      <text class="a-t1">{{item.title1}}</text>
      <text class="a-t2">{{item.title2}}</text>
      <text class="a-t3">{{item.distance}}km</text>
    </view>
    <image src="/pages/tabs/fwts3.png" class="img2" bindtap='tel'></image>
  </view>
  <!-- 点击显示上拉菜单 -->
</view>
